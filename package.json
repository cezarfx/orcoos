{
  "name": "orcoos",
  "version": "0.0.1",
  "description": "Oracle NoSQL DB object modeling for node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "src": "./src",
    "test": "./test",
    "examples": "./examples"
  },
  "files": [
    "README.md",
    "THIRD_PARTY_LICENSES.txt",
    "index.js",
    "index.d.ts",
    "lib/**/*.js",
    "src/**/*.ts",
    "examples/**"
  ],
  "scripts": {
    "start": "ts-node .",
    "build": "tsc",
    "dev": "nodemon --watch src -e ts,js --exec ts-node .",
    "test": "mocha --require ts-node/register ./test/*.test.ts",
    "testw": "mocha --require ts-node/register ./test/*.test.ts --watch-files ./test/ -w",
    "cov": "nyc --reporter=text ts-node ./node_modules/mocha/bin/mocha.js 'test/**/*.test.ts'",
    "m": "npm run build && mocha dist/test/create_drop.test.js"
  },
  "dependencies": {
    "bson": "^6.1.0",
    "kareem": "^2.5.1",
    "mpath": "^0.9.0",
    "mquery": "^5.0.0",
    "oracle-nosqldb": "^5.5.0",
    "package.json": "^0.0.0",
    "sift": "^17.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.11.5",
    "@types/sinon": "^10.0.16",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sinon": "^16.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "nosql",
    "db",
    "schema",
    "model",
    "odm",
    "data",
    "datastore",
    "query",
    "nosql",
    "orm",
    "db"
  ],
  "author": "Cezar Andrei",
  "license": "MIT"
}
